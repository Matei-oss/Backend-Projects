<%- include("partials/header") -%>
    <h1>Home</h1>
    <p>
        <%= startingContent %>
    </p>
    <%      postsList.forEach(function(post){ %>
        <h1>
            <%= post.title %>
        </h1>
        <p>
            <%= post.content.substring(1, 100) + "..."%>
                <a href="/posts/<%= post.title %>">Read More</a>
        </p>

        <% }) %>
            <%- include("partials/footer") -%>